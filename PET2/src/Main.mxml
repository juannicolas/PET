	<fx:Script>
		<![CDATA[
			import air.net.URLMonitor;
			import flash.events.IOErrorEvent;
			import flash.filesystem.*;
			import flash.net.FileFilter;
			import flash.utils.ByteArray;
			import mx.controls.Alert;
			import spark.components.Application;
			import spark.filters.DropShadowFilter;			
			
			
			// Gobal variables start
			protected var csvFile:File;
			protected var shadowFilter:spark.filters.DropShadowFilter;
			protected const fileFilter:FileFilter = new FileFilter("CSV Files","*.csv;");
			protected var monitor:URLMonitor;
			//Global variables end
			
			
			protected function application_creationCompleteHandler():void
			{
				
				// Center main AIR app window on the screen
				nativeWindow.x = (Capabilities.screenResolutionX - nativeWindow.width) / 2;
				nativeWindow.y = (Capabilities.screenResolutionY - nativeWindow.height) / 2;
				
				// Detecting if user is present
				NativeApplication.nativeApplication.idleThreshold = 30;
				NativeApplication.nativeApplication.addEventListener(Event.USER_IDLE, onUserIdle);
				NativeApplication.nativeApplication.addEventListener (Event.USER_PRESENT, onUserPresent);
				
				// Detecting online/offline network connectivity with a URLMonitor
				monitor = new URLMonitor(new URLRequest("https://www.ezpaycenters.net/swspost.asp?")); // change URL to URL desired
				monitor.addEventListener(StatusEvent.STATUS, onStatusChange);
				monitor.pollInterval = 20000;// Milliseconds
				monitor.start();
				
				// creates a generic drop shadow to use on components that don't accept CSS shadow styles
				shadowFilter = new DropShadowFilter();
				shadowFilter.color = 0x000000;
				shadowFilter.alpha = 0.4;
				shadowFilter.blurX = 5;
				shadowFilter.blurY = 5;
				shadowFilter.distance = 5;
				
				// 'external' shadows
				addShadow(this);
			}
			protected function clearEmailFields():void
			{
				emailMsg.text = "";
				emailSubject.text = "";
				emailName.text = "";
				emailUserEmail.text = "";
				cb.selected = false;
				
			}
			protected function addShadow(comp:DisplayObject):void
			{
				comp.filters = [this.shadowFilter];
			}
			protected function closeApp(event:MouseEvent):void 
			{
				this.nativeApplication.exit();
				trace("Application exited by user");
			}		
			protected function onMouseDown(evt:MouseEvent):void
			{
				stage.nativeWindow.startMove();
			}
			protected function emailLinkBtn_clickHandler():void
			{
				const state:String = currentState;
				if ( state == 'main' ) 
				{
					currentState='Main_msgWindow';
				}
				if ( state == 'Main_LogWindow' ) 
				{
					currentState='Main_LogWindow_msgWindow';
				}
			}			
			protected function emailCloseBtn_clickHandler():void
			{
				const state:String = currentState;
				if ( state == 'Main_msgWindow' ) 
				{
					currentState='main';
					clearEmailFields();
				}
				if ( state == 'Main_LogWindow_msgWindow' ) 
				{
					currentState='Main_LogWindow';
					clearEmailFields();
				}
			}
			protected function rollUpBtn_clickHandler():void
			{
				const state:String = currentState;
				if ( state == 'Main_LogWindow' ) 
				{
					currentState='main';
				}
				if ( state == 'Main_LogWindow_msgWindow' ) 
