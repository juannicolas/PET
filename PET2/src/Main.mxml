				}
			}			
			protected function button_clickHandler():void
			{
				const state:String = currentState;
				if ( state == 'main' ) 
				{
					currentState='Main_LogWindow';
				}
				if ( state == 'Main_msgWindow' ) 
				{
					currentState='Main_LogWindow_msgWindow';
				}
			}
			protected function homeLinkBtn_clickHandler():void
			{
				navigateToURL( new URLRequest( encodeURI("http://www.paytechco.com")), "_blank");
			}
			protected function browseCSV(event:Event):void
			{
				csvFile = new File();
				
				// Event Handlers
				csvFile.addEventListener(Event.SELECT, onFileSelected);
				csvFile.addEventListener(Event.CANCEL, onCancel);
				
				// Browse for file
				csvFile.browseForOpen("Select CSV file to upload", [fileFilter]);
				
			}
			protected function onFileSelected(event:Event):void
			{
				// Display file path
				fileNameFld.text = File(event.target).nativePath;
				
				// Event Handler
				csvFile.addEventListener(Event.COMPLETE, loadComplete);
				csvFile.addEventListener(IOErrorEvent.IO_ERROR, onLoadError);
				
				
				// load file into app memory
				csvFile.load();
				logWindow.text = "File loaded into application succesfully : " + File(event.target).name.toUpperCase();
				trace("file loaded into memory");
			}
			protected function loadComplete(event:Event):void
			{
				var fr:String = csvFile.data.toString();
				var split:Array = fr.split("\r");
				var total:Number = 0;
				for (var x:int = 0; x < split.length; x++)
				{
					totalLinesFld.text = x.toString();
					try{
						var split2:Array = split[x].toString().split(",");
						if (split2.length > 1)
						{
							if(x==0)
							{
								deparmentFld.text = split2[0] + " - " + split2[1].toString().toUpperCase();
							}
							else
							{
								var tempt:String = split2[4];
								total += parseFloat(tempt);
							}
							totalAmountFld.text = cFormat.format(total.toString());
						}	
					}
					catch (error:ErrorEvent)
					{
						trace("Error trying to process CSV file line: " + x + "=>" + error.text);
					}
				}
			}
			protected function onCancel(e:Event):void
			{
				trace("User canceled browse");
				csvFile = null;
			}
			protected function onLoadError(e:IOErrorEvent):void
			{
				trace("Error loading file : " + e.text);
			}
			protected function onUserIdle(event:Event):void 
			{
				trace("Handling user idle event");
				this.minimize();
			}
			protected function onUserPresent(event:Event):void
			{
				trace("Handling user present event");
				this.restore();
			}
			protected function onStatusChange(e:StatusEvent):void // Handles when the HTTP Network status has changed.
			{
				if (monitor.available) 
				{
					currentState = 'main';
					trace("Network is online");
					dragMe.addEventListener(MouseEvent.MOUSE_DOWN, onMouseDown);
					netLabel.visible = false;
					enableBtns();					
				} 
				else 
				{
					trace("Network is offline");
					netLabel.visible = true;
					disableBtns();
					
				}
			}
			protected function disableBtns():void
			{
				uploadBtn.enabled = false;
				emailSendBtn.enabled = false;
				trace("Upload & SendEmail Buttons Disabled");
			}
			protected function enableBtns():void
			{
				uploadBtn.enabled = true;
				emailSendBtn.enabled = true;
				trace("Upload & SendEmail Buttons Enabled");
			}
		]]>
	</fx:Script>
	
